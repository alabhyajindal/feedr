<div id='feeds-cont'>
  <% feeds.each do |feed| %>
    <a href="/feed/<%= feed['id'] %>">
      <%= feed['feed_title'] %>
    </a>
    <a href="/feed/<%= feed['id'] %>/edit">
      <button>Edit</button>
    </a>
    <button hx-delete='/feed/<%= feed['id'] %>/delete' 
    hx-confirm='Are you sure you want to delete this feed?'>Delete</button>
  <% end %>
</div>

<a href='/feed/new'>Add new feed</a>
